<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:cv="clr-namespace:BluOsNadRemote.App.ContentViews"
             x:Class="BluOsNadRemote.App.ContentViews.SpectrumAnalyzerView"
             x:DataType="cv:SpectrumAnalyzerView">
    <Grid x:Name="RootGrid" Padding="0,0,0,2" ColumnDefinitions="2*,1*,2*,1*,2*">
        <Border StrokeThickness="0" x:Name="Bar1" Grid.Column="0" VerticalOptions="End" BackgroundColor="White" StrokeShape="RoundRectangle 6 6 0 0" />
        <Border StrokeThickness="0" x:Name="Bar2" Grid.Column="2" VerticalOptions="End" BackgroundColor="White" StrokeShape="RoundRectangle 6 6 0 0" />
        <Border StrokeThickness="0" x:Name="Bar3" Grid.Column="4" VerticalOptions="End" BackgroundColor="White" StrokeShape="RoundRectangle 6 6 0 0" />
    </Grid>
</ContentView>
